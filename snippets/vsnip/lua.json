      "for ${1:i} = ${2:1}, ${3:10} do",
      "\t${0}",
      "end"
    ]
  },
  "foreach": {
    "prefix": "foreach",
    "body": [
      "for ${1:_}, ${2:v} in ipairs(${3}) do",
      "\t${0}",
      "end"
    ]
  },
  "forin": {
    "prefix": "forin",
    "body": [
      "for ${1} in ${2} do",
      "\t${0}",
      "end"
    ]
  },
  "if": {
    "prefix": "if",
    "body": [
      "if ${1} then",
      "\t${0}",
      "end"
    ]
  },
  "else": {
    "prefix": "else",
    "body": [
      "else",
      "\t${0}"
    ]
  },
  "while": {
    "prefix": "while",
    "body": [
      "while ${1} do",
      "\t${0}",
      "end"
    ]
  },
  "do": {
    "prefix": "do",
    "body": [
      "do",
      "\t${0}",
      "end"
    ]
  },
  "lambda": {
    "prefix": "lam",
    "body": [
      "function(${1})",
      "\t${0}",
      "end"
    ]
