snippet     lfecho
abbr        lfCmd("echomsg ...")
alias       echo
    lfCmd("echomsg '{}'".format(${1}))

snippet     writefile
abbr        filewrite,file
    from pathlib import Path
    p = Path(r'~/tmp/leaderf.log')
    with p.open('w', encoding='utf-8') as f:
        f.writelines('hello')

snippet     out
abbr        print()
    print(${0})

snippet     try
abbr        try-except
    try:
        ${0}
    except Exception as e:
        raise e
